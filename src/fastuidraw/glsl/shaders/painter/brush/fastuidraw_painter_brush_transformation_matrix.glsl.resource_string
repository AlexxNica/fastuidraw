void
fastuidraw_gl_brush_main(in uint sub_shader,
                         inout uint shader_data_offset,
                         inout vec2 brush_location,
                         inout vec4 color)
{
  mat2 m;
  shader_data_offset = fastuidraw_read_brush_transformation_matrix(shader_data_offset, m);
  brush_location = m * brush_location;
}
